import java.util.*;
class TestClass {
    public static void main(String args[] ) throws Exception {
        Scanner sc=new Scanner(System.in);
        int x=0,y=0;
        String s=sc.next();
        for(int i=0;i<s.length();i++){
            if(s.charAt(i)=='z'){
                x++;
            }
            else{
                y++;
            }
        }
        if(2*x==y){
            System.out.println("Yes");
        }
        else
        System.out.println("No");
    }
}
